{
    "name": "eugenesh/dns-records",
    "description": "A simple PHP library that allows you to get all DNS records for a specified domain name.",
    "type": "library",
    "require": {
        "php": ">=8.0"
    },
    "autoload": {
        "psr-4": {
            "DnsRecords\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DnsRecords\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Eugene Shalko",
            "email": "eugene.shalko.00@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^10"
    },
    "scripts": {
        "test": "vendor/bin/phpunit --bootstrap vendor/autoload.php tests"
    }
}
